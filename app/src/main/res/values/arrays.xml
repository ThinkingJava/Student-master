<?xml version="1.0" encoding="utf-8"?>
<resources>
    <array name="splach_circle_colors">
        <item>@color/blue</item>
        <item>@color/green</item>
        <item>@color/pink</item>
        <item>@color/orange</item>
        <item>@color/aqua</item>
        <item>@color/yellow</item>
    </array>
    <array name="personaldata">
        <item>学号</item>
        <item>姓名</item>
        <item>年级</item>
        <item>专业</item>
        <item>身份证号</item>
        <item>邮箱</item>
        <item>导师</item>
        <item>辅导员</item>
        <item>已获得</item>
        <item>在读</item>
        <item>预计获得</item>
        <item>绩点</item>
        <item>必修课总学分</item>
        <item>必修课已获得学分</item>
        <item>选修课已获得学分</item>

    </array>
    <attr name="position">
        <enum name="left_top" value="0" />
        <enum name="left_bottom" value="1" />
        <enum name="right_top" value="2" />
        <enum name="right_bottom" value="3" />
    </attr>
    <attr name="radius" format="dimension"></attr>

    <declare-styleable name="AreMenu">
        <attr name="radius"/>
        <attr name="position"/>
    </declare-styleable>
</resources>
